{
  "name": "numcpp",
  "version-string": "2.1.0",
  "description": "C++ implementation of the Python Numpy library",
  "homepage": "https://dpilger26.github.io/NumCpp",
  "license": "MIT",
  "dependencies": [
    "boost-algorithm",
    "boost-endian",
    "boost-integer",
    "boost-math",
    "boost-predef",
    "boost-random",
    "boost-type-traits"
  ],
  "features": {
    "python": {
      "description": "Interacting with python with pybind11 interface",
      "dependencies": [
        "boost-python",
        "pybind11"
      ]
    }
  }
}
